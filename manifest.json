{
  "manifest_version": 2,

  "name": "Jenkins code freeze monitor",
  "description": "This chrome extension monitors code freeze",
  "version": "0.0.0.7",
  "background": {
    "scripts": ["js/background.bundle.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "notifications"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
